#include<stdio.h>
int main(){
int a,b,c;
int x,y;
scanf("%d%d",&a,&b);
x=a;
y=b;
while(b!=0){
c=a%b;
a=b;
b=c;
}
printf("%d",a);}


（递归版)
#include<stdio.h>
int gcd(int a,int b){
if(b==0)
return a;
else
return gcd(b,a%b);
}
int main(){
int a,b;
scanf("%d%d",&a,&b);
if(a<b)
printf("%d\n",gcd(b,a));
else
printf("%d\n",gcd(a,b));
}
