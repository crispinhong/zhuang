#include<stdio.h>
int main(){
	int n,i,s,count;
	double sum=0;
	double average;
	int t=1; 
	printf("please enter:");
	scanf("%d",&n);
	for(i=0;i<n;i++){
		scanf("%d",&s);
		if(s>60){
		t++;	
		}
		sum+=s;
	} 
	average=sum/n;
	count=t;
	printf("average=%.2lf\n",average);
	printf("count=%d",count);	
} 
